{
  "name": "@madulinux/react-datatable",
  "version": "1.0.1",
  "description": "Reusable DataTable component for React with sorting, paging, filter, and export.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "author": "madulinux <npmjs.com/~madulinux>",
  "repository": {
    "type": "git",
    "url": "https://github.com/madulinux/react-datatable"
  },
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "scripts": {
    "test": "jest --passWithNoTests",
    "build": "npm run build:tsup",
    "build:tsup": "tsup --config tsup.config.ts",
    "dev": "tsup --config tsup.config.ts --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "datatable",
    "table",
    "paging",
    "export",
    "component",
    "madulinux"
  ],
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.1",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-popover": "^1.1.13",
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-slot": "^1.2.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.475.0",
    "react": ">=17.0.0",
    "react-day-picker": "^8.10.1",
    "tailwind-merge": "^3.0.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react-dom": "^18.0.0",
    "ts-jest": "^29.3.2",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0"
  }
}
